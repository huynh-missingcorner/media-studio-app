    "/api/media/history": {
      "get": {
        "operationId": "MediaController_getMediaHistory",
        "parameters": [
          {
            "name": "page",
            "required": true,
            "in": "query",
            "description": "Page number (1-based)",
            "schema": {
              "minimum": 1,
              "default": 1,
              "type": "number"
            }
          },
          {
            "name": "limit",
            "required": true,
            "in": "query",
            "description": "Number of items per page",
            "schema": {
              "minimum": 1,
              "maximum": 100,
              "default": 10,
              "type": "number"
            }
          },
          {
            "name": "mediaType",
            "required": false,
            "in": "query",
            "description": "The type of media to filter by",
            "schema": {
              "example": "IMAGE",
              "type": "string"
            }
          },
          {
            "name": "status",
            "required": false,
            "in": "query",
            "description": "The status of the media to filter by",
            "schema": {
              "example": "PENDING",
              "type": "string"
            }
          },
          {
            "name": "search",
            "required": false,
            "in": "query",
            "description": "The search query to filter by",
            "schema": {
              "example": "sunset",
              "type": "string"
            }
          },
          {
            "name": "projectId",
            "required": false,
            "in": "query",
            "description": "The ID of the project to filter by",
            "schema": {
              "example": "123e4567-e89b-12d3-a456-426614174000",
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Media generation history retrieved"
          }
        },
        "security": [
          {
            "bearer": []
          }
        ],
        "summary": "Get history of media generation requests",
        "tags": ["Media"]
      }
    },
